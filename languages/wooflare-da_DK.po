#, php-format
msgid "Found in <a href=\"%s\" target=\"_blank\">Customize &rarr; WooCommerce &rarr; Store Notice</a>."
msgstr "Findes i <a href=\"%s\" target=\"_blank\">Tilpas &rarr; WooCommerce &rarr; Butiksmeddelelse</a>."

